#include <stdio.h>

int main()
{
	int n = 5;
	int m = 4;
	int a[] = {1,0,3,0,0};
	int b =0;
	for(int i =0;i<m;i++)
	{
	    if(a[i] == 0)
	    {
	        b = b+1;
	    }
	}
	
	{
	int count = 1;
	while(b == 0)
	{
	    count = count *b;
	    b--;
	}
	            			// Reading input from STDIN
	printf("possible ways is %d.\n", count);
	}// Writing output to STDOUT
}
