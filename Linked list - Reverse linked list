traverse(int* head)
{
int* temp;
temp = head;
while(temp.next != NULL)
{
if(temp.data % 2 == 0)
{
temp = even(int*temp);
}
else
{
temp = temp.next;
}
}
}

even(int*temp)
{
int* temp;
int* temp1;
temp1 = temp.next;
if(temp1.data%2 == 0)
{
int b;
b = temp.data;
temp.data = temp1.data;
temp1.data = b;
temp = temp1.next;
return temp;
}

print(int* head;
{
int* temp;
temp = head;
while(temp.next!= NULL)
{
print(temp.data);
temp = temp.next;
}
