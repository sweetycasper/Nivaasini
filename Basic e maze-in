#include <stdio.h>

int main(){
	//int x = 0;
	//int y = 0;
	char a[6] = {"L,L,R,D,D,R"};
	for(int i = 0;i<6;i++)
	{
	    int x = 0;
	    int y = 0;
	    if(a[i] == 'L')
	    {
	        x = x-1;
	    }
	    else if(a[i] == 'R')
	    {
	      x = x+1;  
	    }
	    else if(a[i] == 'U')
	    {
	        y = y + 1;
	    }
	    else
	    {
	        y = y - 1;
	    }
	    printf("%d %d\n", x,y);       // Writing output to STDOUT
}
	}
	
  
  
