#include <stdio.h>

int main()
{
	int num =2;
	int a[3] = {2,2,2};
	int b = 0;
	for(int i =0;i<2;i++)
	{
	    if(a[i] == a[i+1])
	    {
	        //continue;
	        b ++;
	       
	    }
	    else if(a[i]<a[i+1])
	    {
	        printf("no");
	        break;
	    }
	    else
	    {
	        printf("no");
	        break;
	    }
	     
	}
	if(b != 0)
	{
	printf("yes");
	}
}	             			
